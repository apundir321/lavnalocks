  <script>
    function myfunc(){
      var a =document.getElementById("mob").value;
      if(isNaN(a)){
        document.getElementById("msg").innerHTML ="<h4>*Enter Only Numeric Value</h4>";
        return false;
      }
      if(a.length<10){
        document.getElementById("msg").innerHTML ="<h4>*Mobile Number Must Be 10 Digits.</h4>";
        return false;
      }
      if((a.charAt(0) != 9) && (a.charAt(0) != 8) && (a.charAt(0) != 7)){
        document.getElementById("msg").innerHTML ="<h4>*Mobile Number Must Start With 9,8 and 7.</h4>";
        return false;
      }
    }
  </script>
  
  
    
